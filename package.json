{
  "name": "vite_fast",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:ts": "tsc --noEmit",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "cd .. && husky front/vue_app/.husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "msw-auto-mock": "npx msw-auto-mock __tests__/openapi.json -o ./__tests__ ",
    "generate:redocly": "npx redocly bundle document/swagger.yml --output ./src/mocks/openapi.json",
    "generate:openapi": "npx openapi-typescript document/swagger.yml -o ./src/shared/api/v1.schema.ts",
    "generate:mock": "npx msw-auto-mock document/swagger.yml -o ./src/mocks"
  },
  "lint-staged": {
    "*.{ts,vue}": "npm run format"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "openapi-fetch": "^0.11.1",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@faker-js/faker": "^8.4.1",
    "@redocly/cli": "^1.19.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/addon-onboarding": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@storybook/vue3": "^8.2.7",
    "@storybook/vue3-vite": "^8.2.7",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^9.1.4",
    "jsdom": "^20.0.3",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.5",
    "msw-auto-mock": "^0.25.0",
    "msw-storybook-addon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.3.0",
    "postcss": "^8.4.20",
    "prettier": "^2.7.1",
    "storybook": "^8.2.7",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.0.0",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
